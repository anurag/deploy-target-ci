{
  "extends": ["config:base"],
  "packageRules": [
    {
      "automerge": true,
      "followTag": "canary",
      "matchPackageNames": [ "@redwoodjs/core", "@redwoodjs/web", "@redwoodjs/router", "@redwoodjs/forms", "@redwoodjs/auth", "@redwoodjs/api", "@redwoodjs/graphql-server" ]
    }
  ],
  "ignoreDeps": [
    "prop-types",
    "react",
    "react-dom",
    "autoprefixer",
    "postcss",
    "postcss-loader",
    "tailwindcss",
    "@vercel/nft",
    "archiver",
    "fs-extra",
    "serverless",
    "serverless-lift"
  ]
}
